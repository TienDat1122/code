#include <iostream>
#define MAX 100
#define TRUE 1
#define FALSE 0

int n, k, X[MAX], dem = 0, OK = TRUE;
using namespace std;
void Init(void) {
    cout << "\n Nhap n=";
    cin >> n;
    k = 1;
    X[k] = n;
}

void Result(void) {
    cout << "\n Cach chia " << ++dem << ":";
    for (int i = 1; i <= k; i++)
        cout << X[i] << " ";
}

void Next_Division(void) {
    int i = k, j, R, S, D;
    while (i > 0 && X[i] == 1) i--;
    if (i > 0) {
        X[i] = X[i] - 1;
        D = k - i + 1;
        R = D / X[i];
        S = D % X[i];
        k = i;
        if (R > 0) {
            for (j = i + 1; j <= i + R; j++)
                X[j] = X[i];
            k = k + R;
        }
        if (S > 0) {
            k = k + 1;
            X[k] = S;
        }
    }
    else OK = 0;
}

int main() {
    Init(); //Nháº­p n = 5.
    while (OK) {
        Result();
        Next_Division();
    }
    return 0;
}
